{
  "manifest_version": 2,
  "name": "sort text",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery.js", "content.js", "mergeSort.js", "popup.js"]
    }
  ],
  "browser_action": {
    "default_icon": "favicons.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
